{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Active card_ui Module",
            "type": "debugpy",
            "request": "launch",
            "module": "card_ui.${fileBasenameNoExtension}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "justMyCode": true
        }
    ]
}